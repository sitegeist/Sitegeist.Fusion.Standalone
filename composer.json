    "type": "library",
    "license": "GPL-3.0+",
    "require": {
        "sitegeist/eel-standalone": "*",
        "neos/utility-arrays": "*",
        "neos/utility-files": "*",
        "neos/utility-objecthandling": "*"
    },
    "autoload": {
